{
    "name": "php-interactor/php-interactor",
    "type": "library",
    "description": "An Interactor library for PHP",
    "keywords": ["interactor"],
    "homepage": "https://github.com/mbadolato/php-interactor",
    "license": "MIT",
    "authors": [
        {
            "name": "Mark Badolato",
            "email": "mbadolato@cybernox.com",
            "homepage": "http://www.markbadolato.com",
            "role": "Developer"
        }
    ],
    "support": {
        "issues": "https://github.com/mbadolato/php-interactor/issues"
    },
    "require": {
        "php": ">=5.4",
        "symfony/finder": "@stable",
        "phpcollection/phpcollection": "@stable"
    },
    "require-dev": {
        "fabpot/php-cs-fixer": "@stable",
        "phploc/phploc": "@stable",
        "phpmd/phpmd": "@stable",
        "phpspec/prophecy-phpunit": "@stable",
        "phpunit/phpunit": "@stable",
        "squizlabs/php_codesniffer": "@stable"
    },
    "config": {
        "bin-dir": "bin"
    },
    "autoload": {
        "psr-0": {
            "PhpInteractor\\": ["src/", "tests/"]
        }
    },
    "minimum-stability": "stable"
}
